{
    "name"            : "Gerar CPF",
    "version"         : "1.0",
    "description"     : "Gerador de CPF",
    "manifest_version": 2,
    "icons"           : {
        "16": "images/icon16.png",
        "24": "images/icon24.png",
        "32": "images/icon32.png"
